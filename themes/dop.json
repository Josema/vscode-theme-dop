{
    "name": "dop",
    "tokenColors": [
        {
            // "settings": {
            //     "background": "#1e1e1e",
            //     "foreground": "#DDDDDD"
            // }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#CFE36D"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5f6367"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#EBBB84"
            }
        },
        {
          "name": "Boolean",
          "scope": "constant.language.boolean",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#1FA1F1"
          }
        },
        {
          "name": "Number",
          "scope": "constant.numeric",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#1FA1F1"
          }
        },
        {
          "name": "Built-in constant",
          "scope": "constant.language",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#1FA1F1"
          }
        },
        {
          "name": "User-defined constant",
          "scope": "constant.character, constant.other",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#1FA1F1"
          }
        },
        {
          "name": "Storage",
          "scope": "storage.type, storage.modifier",
          "settings": {
            "foreground": "#E12977",
            "fontStyle": "bold italic"
            // "foreground": "#fa6981"
          }
        },
        {
          "name": "Function argument",
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "bold italic",
            "background": "#000000",
            "foreground": "#FFFFFF"
          }
        },
        {
            "name": "Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#E12977"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#1FA1F1"
            }
        },
        {
          "name": "Library function",
          "scope": "support.function",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#FFAA00"
          }
        },
        {
          "name": "Library class",
          "scope": "support.type",
          "settings": {
            "foreground": "#FFAA00"
          }
        },
        {
          "name": "Library variable",
          "scope": "support.class",
          "settings": {
            "foreground": "#FFAA00"
          }
        },
        {
          "name": "JSON key",
          "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#CCCCCC"
          }
        },
        {
          "name": "JSON constant",
          "scope": "constant.language.json",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#1fa1f1"
          }
        },
        {
          "name": "CSS class",
          "scope": "entity.other.attribute-name.class",
          "settings": {
            "foreground": "#FFCB6B"
          }
        },
        {
          "name": "CSS ID",
          "scope": "entity.other.attribute-name.id",
          "settings": {
            "foreground": "#FFCB6B"
          }
        },
        {
          "name": "CSS tag",
          "scope": "source.css entity.name.tag",
          "settings": {
            "foreground": "#FA6981"
          }
        },
        {
          "name": "CSS property",
          "scope": "support.type.property-name.css",
          "settings": {
            "foreground": "#e5d0b1"
          }
        },
        {
            "name": "CSS Units",
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#FFAA00"
            }
        },
        {
          "name": "HTML tag outer",
          "scope": "meta.tag, punctuation.definition.tag",
          "settings": {
            "foreground": "#DDDDDD"
          }
        },
        {
          "name": "HTML tag inner",
          "scope": "entity.name.tag",
          "settings": {
            "foreground": "#E12977"
          }
        },
        {
          "name": "HTML tag attribute",
          "scope": "entity.other.attribute-name",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#CFE36D"
          }
        }
    ],
    "colors": {
